<html lang="en" xml:lang="en">
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Ted Zingrebe | tedzingrebe.com</title>
	<meta property="og:title" content="Ted Zingrebe | tedzingrebe.com" />
	<meta property="og:type" content="website" />
	<meta property="og:url" content="http://www.tedzingrebe.com" />
	<meta property="og:description" content="Ted Zingrebe's media portfolio specializing in project/product management, marketing, web and video production." />
	<meta property="og:locale" content="en_US" />
</head>
<body>

<div id="loginHead" class="wrap">
	&nbsp;
</div>
<div id="contentWrap" style="background-image:url(http://tzportfolio.s3.amazonaws.com/backgrounds/bg_toolShop.jpg);">
	<div id="loginWrap" class="wrap">
		<div id="loginContent">
			<div id="login">
				<h2>Log in</h2>
				<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
				  <div><%= f.label :email,  :class => "label" %><br />
				  <%= f.email_field :email, autofocus: true,  :class => "input" %></div>

				  <div><%= f.label :password,  :class => "label" %><br />
				    <%= f.password_field :password, autocomplete: "off",  :class => "input" %></div>

				  <% if devise_mapping.rememberable? -%>
				    <div><%= f.check_box :remember_me %> <%= f.label :remember_me ,  :class => "label"%></div>
				  <% end -%>

				  <div><%= f.submit "Log in" %></div>
				<% end %>


				  <% if user_signed_in? %>

						<li><%= link_to "Log out", destroy_user_session_path, method: :delete %></li>

					<% end %> 	
			</div>
		</div>
	</div>
</div>

<div id="loginFoot" class="wrap">
<p>ted zingrebe | 2014
</div>

</body>
</html>