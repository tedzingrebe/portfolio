							<% end %>
							</li>
							<li>
							<%= link_to landing_path(@landing, :anchor => "aProjects") do %>
							<img class="mobileIcon" src="http://tzportfolio.s3.amazonaws.com/icons/i-3.png">
								Projects
							<% end %>
							</li>
							<li>
							<%= link_to landing_path(@landing, :anchor => "aProducts") do %>
							<img class="mobileIcon" src="http://tzportfolio.s3.amazonaws.com/icons/i-3.png">
